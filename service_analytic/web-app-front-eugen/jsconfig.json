{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~/*": ["./*"],
            "@/*": ["./*"],
            "~~/*": ["./*"],
            "@@/*": ["./*"],
            "~use/*": ["./assets/js/use/*"],
            "~directives/*": ["./assets/js/directives/*"],
            "~utils/*": ["./assets/js/utils/*"],
            "~mixins/*": ["./assets/js/mixins/*"]
        }
    },
    "exclude": ["node_modules", ".nuxt", "dist"]
}
